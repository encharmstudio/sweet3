{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "space-in-parens": ["error", "never"],
    "indent": ["error", 2, { "VariableDeclarator": "first" }],
    "new-parens": "error",
    "array-bracket-spacing": ["error", "never"],
    "computed-property-spacing": ["error", "never"]
  }
}
